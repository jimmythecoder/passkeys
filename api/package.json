        "esbuild-plugin-fileloc": "^0.0.6",
        "express": "^4.19.2",
        "fastify": "^4.26.2",
        "fastify-plugin": "^4.5.1",
        "helmet": "^7.1.0",
        "jose": "^5.2.3",
        "json-schema-to-ts": "^3.0.1",
