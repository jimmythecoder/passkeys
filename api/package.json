{
    "name": "@passkeys/api",
    "displayName": "Passkey API Server",
    "private": true,
    "version": "1.0.0",
    "description": "Fastify / express server to handle webauthn registration and authentication",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "start": "NODE_ENV=development tsx watch src/index.ts",
        "build:tsc": "tsc && tsc-alias",
        "build:cjs": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.cjs",
        "build": "node build.js",
        "genkey": "tsx utils/genkey.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
